#plot
import numpy as np
import pandas as pd
pre=np.array((1,2,1,1))
label=np.array((1,3,4,5))
mse=(1/len(label))*np.sum(np.square(pre-label))
print('mse {}'.format(mse))
#绘制sinx,cosx,tanx图像
import matplotlib.pyplot as plt
x=np.arange(0,2*np.pi,0.1)
y1=np.sin(x)
y2=np.cos(x)
y3=np.tan(x)
plt.plot(x,y1,'bo--')#颜色+点+线条
plt.plot(x,y2,'r.:')
plt.show()
